#include <stdio.h>
#include <stdlib.h>

struct lista {
    int info;
    struct lista* prox;
    struct lista* ant;
};
typedef struct lista Lista;

Lista* inicializa_lista(void) {
    return NULL;
}
Lista* insere_inicio(Lista* alfa, int num) {
    Lista* novo = (Lista*)malloc(sizeof(Lista));
    novo->info = num;
    novo->ant = NULL;
    novo->prox = alfa;
    if(alfa != NULL) {
        alfa->ant = novo;
    }
    alfa = novo;
    return alfa;
}
Lista* insere_final(Lista* alfa, int num) {
    Lista* novo = (Lista*)malloc(sizeof(Lista));
    novo->info = num;
    novo->prox = NULL;
    Lista* atual = alfa;
    while(atual->prox != NULL) {
        atual = atual->prox;
    }
    atual->prox = novo;
    novo->ant = atual;
    return novo;
}

int main(void) {
    int extremo, num;
    printf("Extremo a ser inserido:\n1) Inicio\n2) Final");
    scanf("%d", &extremo);
    printf("Qual o valor a ser inserido?\n");
    scanf("%d", &num);
    return 0;
}
